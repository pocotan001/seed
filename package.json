{
  "name": "seed",
  "version": "0.0.0",
  "description": "Starter skeleton for frontend projects",
  "private": true,
  "engines": {
    "node": "10"
  },
  "scripts": {
    "start": "node dist/server.js",
    "test": "ENV=test jest",
    "test:watch": "npm test -- --watch --notify",
    "test:update": "npm test -- --updateSnapshot",
    "test:cover": "npm test -- --coverage",
    "script": "babel-node --extensions .ts --presets @babel/env tools/main",
    "dev": "npm run script -- dev",
    "build": "npm run script -- build",
    "build:stats": "ENV=production npm run build -- --analyze",
    "format": "prettier --parser typescript --write '{,!(node_modules)/**/}!(*.d.){ts,tsx}'",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "tslint '{,!(node_modules)/**/}!(*.d.){ts,tsx}'",
    "precommit": "npm run typecheck && lint-staged"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "lint-staged": {
    "!(*.d.){ts,tsx}": [
      "prettier --parser typescript --write",
      "tslint",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "/test/.*\\.test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^~/assets/img/(.+)": "<rootDir>/test/mocks/image"
    },
    "setupFiles": [
      "<rootDir>/test/setup.ts"
    ]
  },
  "dependencies": {
    "@types/react-router-config": "1.1.0",
    "body-parser": "1.18.3",
    "chalk": "2.4.2",
    "connected-react-router": "6.2.2",
    "consola": "2.3.2",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "helmet": "3.15.0",
    "history": "4.7.2",
    "ky": "0.7.0",
    "normalize.css": "8.0.1",
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2",
    "react-helmet": "5.2.0",
    "react-redux": "6.0.0",
    "react-router": "4.3.1",
    "react-router-config": "4.4.0-beta.6",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.7",
    "redux-form": "8.1.0",
    "redux-persist": "5.10.0",
    "rendertron-middleware": "0.1.5",
    "source-map-support": "0.5.10",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.1",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-react-constant-elements": "7.2.0",
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@types/body-parser": "1.17.0",
    "@types/chalk": "2.2.0",
    "@types/copy-webpack-plugin": "4.4.2",
    "@types/dotenv": "6.1.0",
    "@types/express": "4.16.1",
    "@types/helmet": "0.0.42",
    "@types/html-webpack-plugin": "3.2.0",
    "@types/jest": "23.3.13",
    "@types/morgan": "1.7.35",
    "@types/node": "10.12.18",
    "@types/react": "16.7.20",
    "@types/react-cache": "2.0.0",
    "@types/react-dom": "16.0.11",
    "@types/react-helmet": "5.0.8",
    "@types/react-redux": "7.0.0",
    "@types/react-router": "4.4.3",
    "@types/react-router-dom": "4.3.1",
    "@types/react-test-renderer": "16.0.3",
    "@types/redux": "3.6.0",
    "@types/redux-form": "7.4.16",
    "@types/redux-persist": "4.3.1",
    "@types/rimraf": "2.0.2",
    "@types/styled-components": "4.1.6",
    "@types/webpack": "4.4.24",
    "@types/webpack-bundle-analyzer": "2.13.0",
    "@types/webpack-dev-middleware": "2.0.2",
    "@types/webpack-env": "1.13.6",
    "@types/webpack-hot-middleware": "2.16.4",
    "@types/webpack-node-externals": "1.6.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-preval": "3.0.1",
    "babel-plugin-styled-components": "1.10.0",
    "copy-webpack-plugin": "4.6.0",
    "flux-standard-action": "2.0.3",
    "hard-source-webpack-plugin": "0.13.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "morgan": "1.9.1",
    "prettier": "1.16.1",
    "react-test-renderer": "16.7.0",
    "rimraf": "2.6.3",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "tslint-consistent-codestyle": "1.15.0",
    "tslint-microsoft-contrib": "6.0.0",
    "tslint-react": "3.6.0",
    "typescript": "3.2.4",
    "webpack": "4.28.3",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-dev-middleware": "3.5.1",
    "webpack-hot-middleware": "2.24.3",
    "webpack-node-externals": "1.7.2",
    "workbox-webpack-plugin": "3.6.3"
  }
}
