{
  "name": "seed",
  "version": "0.0.2",
  "description": "Starter skeleton for frontend projects",
  "private": true,
  "engines": {
    "node": "8.x.x"
  },
  "scripts": {
    "start": "node dist/server.js",
    "test": "ENV=test jest",
    "test:watch": "npm test -- --watch --notify",
    "test:update": "npm test -- --updateSnapshot",
    "test:cover": "npm test -- --coverage",
    "test:ci": "npm test -- --maxWorkers=2",
    "script": "babel-node --extensions .ts tools/run",
    "dev": "npm run script -- dev",
    "build": "npm run script -- build",
    "build:development": "ENV=development npm run build",
    "build:staging": "ENV=staging npm run build",
    "build:production": "ENV=production npm run build",
    "build:stats": "ENV=production npm run build -- --analyze",
    "format": "prettier --parser typescript --write '{,!(node_modules)/**/}!(*.d.){ts,tsx}'",
    "lint": "tslint '{,!(node_modules)/**/}!(*.d.){ts,tsx}'",
    "typecheck": "tsc",
    "precommit": "lint-staged && tsc --project ."
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "lint-staged": {
    "!(*.d.){ts,tsx}": [
      "prettier --parser typescript --write",
      "tslint",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "/test/.*\\.test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^~/assets/img/(.+)": "<rootDir>/test/mocks/image",
      "^~/(.+)": "<rootDir>/src/$1"
    },
    "setupFiles": [
      "<rootDir>/test/setup.ts"
    ]
  },
  "dependencies": {
    "axios": "0.18.0",
    "axios-retry": "3.1.1",
    "body-parser": "1.18.3",
    "chalk": "^2.4.1",
    "cookie-session": "2.0.0-beta.3",
    "core-js": "2.5.7",
    "debug": "3.1.0",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "final-form": "4.10.0",
    "final-form-focus": "1.1.0",
    "focus-trap": "2.4.6",
    "helmet": "3.13.0",
    "history": "4.7.2",
    "intersection-observer": "0.5.0",
    "lodash": "4.17.10",
    "lru-cache": "4.1.3",
    "marked": "0.5.0",
    "mobx": "5.1.0",
    "mobx-react": "5.2.8",
    "morgan": "1.9.0",
    "normalize.css": "8.0.0",
    "normalizr": "3.2.4",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-final-form": "3.6.5",
    "react-router": "4.3.1",
    "regenerator-runtime": "0.12.1",
    "styled-components": "3.4.2"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/node": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-decorators": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-react-constant-elements": "7.0.0",
    "@babel/plugin-transform-react-inline-elements": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.0.0",
    "@types/body-parser": "1.17.0",
    "@types/chalk": "2.2.0",
    "@types/chokidar": "1.7.5",
    "@types/cookie-session": "2.0.35",
    "@types/debug": "0.0.30",
    "@types/dotenv": "4.0.3",
    "@types/express": "4.16.0",
    "@types/fs-extra": "5.0.4",
    "@types/helmet": "0.0.40",
    "@types/history": "4.7.0",
    "@types/jest": "23.3.1",
    "@types/lodash": "4.14.116",
    "@types/lru-cache": "4.1.1",
    "@types/morgan": "1.7.35",
    "@types/node": "10.9.4",
    "@types/react": "16.4.14",
    "@types/react-dom": "16.0.7",
    "@types/react-router": "4.0.31",
    "@types/react-test-renderer": "16.0.2",
    "@types/webpack": "4.4.11",
    "@types/webpack-bundle-analyzer": "2.9.2",
    "@types/webpack-dev-middleware": "2.0.2",
    "@types/webpack-env": "1.13.6",
    "@types/webpack-hot-middleware": "2.16.4",
    "@types/webpack-manifest-plugin": "1.3.2",
    "@types/webpack-node-externals": "1.6.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.4.2",
    "babel-loader": "8.0.2",
    "babel-plugin-dynamic-import-node": "2.1.0",
    "babel-plugin-preval": "3.0.0",
    "babel-plugin-styled-components": "1.6.1",
    "babel-plugin-transform-imports": "1.5.0",
    "chokidar": "2.0.4",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "hard-source-webpack-plugin": "0.12.0",
    "husky": "0.14.3",
    "jest": "23.5.0",
    "lint-staged": "7.2.2",
    "prettier": "1.14.2",
    "react-test-renderer": "16.5.2",
    "source-map-support": "0.5.9",
    "string-replace-loader": "2.1.1",
    "svg-inline-loader": "0.8.0",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-consistent-codestyle": "1.13.3",
    "tslint-microsoft-contrib": "5.2.1",
    "tslint-react": "3.6.0",
    "typescript": "3.0.3",
    "url-loader": "1.1.1",
    "webpack": "4.17.2",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-dev-middleware": "3.2.0",
    "webpack-hot-middleware": "2.23.1",
    "webpack-manifest-plugin": "2.0.3",
    "webpack-node-externals": "1.7.2",
    "workbox-webpack-plugin": "3.4.1"
  }
}
