{
  "name": "seed",
  "version": "0.0.2",
  "description": "Starter skeleton for frontend projects",
  "private": true,
  "engines": {
    "node": "8.x.x"
  },
  "scripts": {
    "start": "node dist/server.js",
    "test": "jest",
    "dev": "ts-node tools/run dev",
    "build": "ts-node tools/run build",
    "lint": "tslint '{src,tools}/!(typings)/**/*.{ts,tsx}'",
    "prettier": "prettier --parser typescript --write '{src,tools}/!(typings)/**/*.{ts,tsx}'",
    "precommit": "lint-staged",
    "deploy": "NODE_ENV=production npm run build && gcloud app deploy --project [PROJECT_ID]"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/test/.*\\.test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^~/(.+)": "<rootDir>/src/$1"
    },
    "setupFiles": [
      "<rootDir>/test/setup.ts"
    ]
  },
  "lint-staged": {
    "{src,tools}/!(typings)/**/*.{ts,tsx}": [
      "prettier --parser typescript --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0-beta.52",
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "chalk": "^2.4.1",
    "cookie-session": "2.0.0-beta.3",
    "debug": "3.1.0",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "helmet": "3.12.1",
    "history": "4.7.2",
    "intersection-observer": "0.5.0",
    "lodash": "4.17.10",
    "marked": "0.4.0",
    "mobx": "5.0.3",
    "mobx-react": "5.2.3",
    "morgan": "1.9.0",
    "normalize.css": "8.0.0",
    "normalizr": "3.2.4",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-router": "4.3.1",
    "styled-components": "3.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.52",
    "@babel/plugin-transform-react-constant-elements": "7.0.0-beta.52",
    "@babel/plugin-transform-react-inline-elements": "7.0.0-beta.52",
    "@babel/preset-env": "7.0.0-beta.52",
    "@babel/preset-react": "7.0.0-beta.52",
    "@babel/preset-stage-2": "7.0.0-beta.52",
    "@types/body-parser": "1.17.0",
    "@types/chalk": "2.2.0",
    "@types/chokidar": "1.7.5",
    "@types/cookie-session": "2.0.34",
    "@types/debug": "0.0.30",
    "@types/dotenv": "4.0.3",
    "@types/express": "4.11.1",
    "@types/fs-extra": "5.0.2",
    "@types/helmet": "0.0.38",
    "@types/history": "4.6.2",
    "@types/jest": "23.1.2",
    "@types/lodash": "4.14.110",
    "@types/morgan": "1.7.35",
    "@types/node": "10.5.2",
    "@types/react": "16.4.6",
    "@types/react-dom": "16.0.6",
    "@types/react-router": "4.0.28",
    "@types/react-test-renderer": "16.0.1",
    "@types/webpack": "4.1.5",
    "@types/webpack-dev-middleware": "2.0.1",
    "@types/webpack-env": "1.13.6",
    "@types/webpack-hot-middleware": "2.16.3",
    "@types/webpack-manifest-plugin": "1.3.2",
    "@types/webpack-merge": "4.1.3",
    "@types/webpack-node-externals": "1.6.3",
    "babel-loader": "8.0.0-beta.4",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-preval": "3.0.0",
    "babel-plugin-styled-components": "1.5.1",
    "chokidar": "2.0.4",
    "file-loader": "1.1.11",
    "fs-extra": "6.0.1",
    "hard-source-webpack-plugin": "0.8.1",
    "husky": "0.14.3",
    "jest": "23.3.0",
    "lint-staged": "7.2.0",
    "prettier": "1.12.1",
    "react-test-renderer": "16.4.1",
    "string-replace-loader": "2.1.1",
    "svg-inline-loader": "0.8.0",
    "ts-jest": "23.0.0",
    "ts-loader": "4.4.2",
    "ts-node": "6.2.0",
    "tslint": "5.10.0",
    "tslint-config-prettier": "1.13.0",
    "tslint-react": "3.6.0",
    "typescript": "2.9.2",
    "url-loader": "1.0.1",
    "webpack": "4.6.0",
    "webpack-dev-middleware": "3.1.2",
    "webpack-hot-middleware": "2.22.1",
    "webpack-manifest-plugin": "2.0.3",
    "webpack-merge": "4.1.2",
    "webpack-node-externals": "1.7.2",
    "workbox-webpack-plugin": "3.3.0"
  }
}
